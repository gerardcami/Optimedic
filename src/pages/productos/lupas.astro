---
import Layout from '../../layouts/Layout.astro'
import ProductTitle from '../../components/ProductTitle.astro'
import Products from '../../components/products/loupes/Products.astro'

import { getI18N } from '../../i18n/'
const { currentLocale = 'es' } = Astro

const i18n = getI18N({ currentLocale }).CONTACT_PAGE
const seoData = i18n.SEO
---

<Layout seoData={seoData}>
	<ProductTitle productType="LOUPES" />
	<Products productType="LOUPES" />
</Layout>
