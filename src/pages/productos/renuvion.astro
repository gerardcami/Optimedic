---
import Layout from '../../layouts/Layout.astro'

import { getI18N } from '../../i18n/'
import { SurgeryHeading } from '../../components/products/surgery/surgery-heading'
import { RenuvionSteps } from '../../components/products/surgery/renuvion/renuvion-steps'
import RealResults from '../../components/products/surgery/renuvion/RealResults.astro'
import BudgetRequest from '../../components/BudgetRequest.astro'
const { currentLocale = 'es' } = Astro

const i18n = getI18N({ currentLocale }).RENUVION_PAGE
const seoData = i18n.SEO
const content = i18n
---

<Layout seoData={seoData}>
	<SurgeryHeading content={content.heading} />
	<RenuvionSteps content={content.steps} />
	<RealResults />
	<BudgetRequest />
</Layout>
